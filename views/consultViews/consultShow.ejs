<% var deleteLink = consultation.id + '?_method=DELETE' %>
<div class="container">
  <pre>
    <%- consultation %>
  </pre>
  <form class="" action="edit" method="get">
    <button type="submit" name="id" value="<%- consultation.id %>">Edit</button>
  </form>
  <form class="" action="<%- deleteLink %>" method="POST">
    <button type="submit" name="id" value="<%- consultation.id %>">Delete</button>
  </form>
</div>
